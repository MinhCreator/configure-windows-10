{
    "workbench.iconTheme": "vscode-icons",
    
    "files.autoSave": "afterDelay",
    
    "editor.fontSize": 13,
    
    "editor.fontFamily": "'Cascadia Code'",
    
    "files.exclude": {
        "**/.": true
    },
    
    "editor.fontLigatures": true,
    
    "editor.codeLens": false,
    
    "editor.codeActionWidget.showHeaders": false,
    
    "editor.minimap.enabled": false,
    
    "editor.minimap.renderCharacters": false,
    
    "window.autoDetectHighContrast": false,
    
    "extensions.autoUpdate": false,
    
    "terminal.integrated.fontSize": 13,
    
    "update.enableWindowsBackgroundUpdates": false,
    
    "security.workspace.trust.enabled": false,
    
    "security.workspace.trust.banner": "never",
    
    "security.workspace.trust.startupPrompt": "never",
    
    "python.analysis.autoFormatStrings": true,
    
    "python.analysis.autoImportCompletions": true,
    
    "python.analysis.addImport.exactMatchOnly": true,
    
    "python.analysis.completeFunctionParens": true,
    
    "editor.detectIndentation": false,
    
    "editor.accessibilitySupport": "off",
    
    "update.mode": "manual",
    
    "markdown.updateLinksOnFileMove.enabled": "always",
    
    "terminal.integrated.tabs.enableAnimation": false,
    
    "workbench.reduceMotion": "on",
    
    "editor.wordWrap": "on",
    
    "editor.cursorBlinking": "smooth",
    
    "workbench.colorCustomizations": {

    },
    
    "editor.bracketPairColorization.enabled": true,
    
    "editor.matchBrackets": "never",
    
    "workbench.startupEditor": "none",
    
    "terminal.integrated.cursorStyle": "line",
    
    "terminal.integrated.cursorBlinking": true,
    
    "editor.scrollbar.horizontalScrollbarSize": 11,
    
    "editor.scrollbar.verticalScrollbarSize": 12,
    
    "explorer.confirmDragAndDrop": false,
    
    "[html]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    
    "editor.guides.bracketPairs": "active",
    
    "liveServer.settings.donotShowInfoMsg": true,
    
    "workbench.productIconTheme": "fluent-icons",
    
    "editor.tabCompletion": "on",

    "python.languageServer": "Pylance",
    
    "workbench.editor.enablePreview": false,
    
    "workbench.tree.indent": 10,
    
    "workbench.colorTheme": "2077",
    
    "window.commandCenter": false,
    
    "workbench.editor.tabSizing": "fixed",
    "screencastMode.fontSize": 20,
    "screencastMode.keyboardOverlayTimeout": 500,
    "screencastMode.mouseIndicatorColor": "#00FFFFFF",
    "screencastMode.keyboardShortcutsFormat": "commandWithGroupAndKeys",
    "screencastMode.hideSingleEditorCursorMoves": true,
    "explorer.confirmDelete": false,
    "notebook.output.wordWrap": true,
    "vsicons.dontShowNewVersionMessage": true,
    "git.confirmSync": false,
    "workbench.statusBar.visible": false,
    "hediet.vscode-drawio.resizeImages": null,
    "workbench.sideBar.location": "right",
    "errorLens.gutterIconsEnabled": true,
    "errorLens.gutterIconSet": "defaultOutline",
    "errorLens.gutterIconSize": "auto",
    "breadcrumbs.icons": false
}